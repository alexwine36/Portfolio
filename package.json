{
  "name": "portfolio",
  "version": "1.9.0",
  "license": "MIT",
  "scripts": {
    "commit": "run-s commit:*",
    "commit:cz": "git cz -a",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --target=test --all",
    "test:ci": "nx run-many --target=test --all --parallel=3 --ci --code-coverage --coverageReporters=cobertura --coverageReporters=html --coverageReporters=text",
    "lint": "nx affected --target=lint --parallel=3",
    "format:check": "nx format:check",
    "doc:build": "nx build docs",
    "doc:start": "nx serve docs",
    "doc:version": "docusaurus --config apps/docs/docusaurus.config.js docs:version 1.0"
  },
  "private": true,
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.13",
    "@docusaurus/preset-classic": "2.0.0-beta.13",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/react": "^1.6.21",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.3",
    "@swc/helpers": "~0.3.3",
    "clsx": "^1.1.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.11.3",
    "js-base64": "^3.7.2",
    "prism-react-renderer": "^1.2.1",
    "random": "^3.0.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.4.0",
    "react-parallax": "^3.5.0",
    "react-router-dom": "6",
    "react-sizeme": "^3.0.2",
    "regenerator-runtime": "0.13.7",
    "seedrandom": "^3.0.5",
    "simpler-color": "^1.0.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@docusaurus/module-type-aliases": "2.0.0-beta.13",
    "@emotion/babel-plugin": "11.9.2",
    "@nrwl/cli": "14.3.1",
    "@nrwl/cypress": "14.3.1",
    "@nrwl/eslint-plugin-nx": "14.3.1",
    "@nrwl/jest": "14.3.1",
    "@nrwl/linter": "14.3.1",
    "@nrwl/nx-cloud": "14.1.0",
    "@nrwl/react": "14.3.1",
    "@nrwl/web": "14.3.1",
    "@nrwl/workspace": "14.3.1",
    "@nx-plus/docusaurus": "^14.0.0",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@swc/jest": "0.2.20",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/seedrandom": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "babel-jest": "27.5.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.0",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
