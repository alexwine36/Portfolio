{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "portfolio",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "MmYzZTNiNjYtMTNmMi00M2JjLWJiMmUtNWRiMDEyNmExMWFkfHJlYWQtd3JpdGU="
      }
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "@emotion/styled"
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
